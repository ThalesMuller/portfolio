{
    "files.eol": "\n",
    "editor.formatOnSave": true,
    "files.exclude": {
        ".env": true,
        ".eslintignore": true,
        ".gitattributes": true,
        "build/": true,
        "config/": true,
        "coverage/": true,
        "deps/": true,
        "jest.config.js": true,
        "node_modules/": true,
        "package-lock.json": true,
        "public/": true,
        "scripts/": true,
        "yarn.lock": true,
        "yarn-error.log": true,
    },
    "typescript.suggest.autoImports": true,
    "typescript.preferences.importModuleSpecifier": "shortest",
}